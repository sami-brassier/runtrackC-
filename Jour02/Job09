#include <iostream>
using namespace std;

int main() {
    int a, b, entier;

    cout << "Entrez la borne inférieure a : ";
    cin >> a;
    cout << "Entrez la borne supérieure b : ";
    cin >> b;

    if (a >= b) {
        cout << "Erreur : a doit être strictement inférieur à b." << endl;
        return 1;
    }

    cout << "Entrez un entier : ";
    cin >> entier;

    if (entier >= a && entier <= b) {
        cout << "GAGNE" << endl;
    } else {
        cout << "PERDU" << endl;
    }

    return 0;
}
