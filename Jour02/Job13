#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    cout << "   I";
    for (int i = 1; i <= 10; ++i) {
        cout << setw(4) << i;
    }
    cout << endl;
    cout << "---------------------------------------" << endl;

    // Afficher le corps de la table
    for (int i = 1; i <= 10; ++i) {
        cout << setw(2) << i << " I"; // Afficher la ligne et le sÃ©parateur
        for (int j = 1; j <= 10; ++j) {
            cout << setw(4) << i * j; // Calculer et afficher le produit
        }
        cout << endl;
    }

    return 0;
}
