#include <iostream>
using namespace std;

int main() {
    int a, b;

    cout << "Entrez la borne inférieure a : ";
    cin >> a;
    cout << "Entrez la borne supérieure b : ";
    cin >> b;

    cout << "Les entiers de " << a << " à " << b << " sont : ";
    int i = a;
    while (i <= b) {
        cout << i << " ";
        i++;
    }
    cout << endl;

    return 0;
}
