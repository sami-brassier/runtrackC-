#include <iostream>

int main() {
    int total = 0;
    int entier = 0;
    for (int i = 0; i < 5; ++i) {
        std::cout << "Entrez un entier : ";
        std::cin >> entier;
        total += entier;
    }
    double moyenne = static_cast<double>(total) / 5;
    std::cout << "La moyenne des cinq entiers est : " << moyenne << std::endl;
    return 0;
}
