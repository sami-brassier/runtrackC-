#include <iostream>
#include <string>
using namespace std;

struct Staff {
    string nom;
    string prenom;
};

struct Etudiant {
    string nom;
    string prenom;
};

int main() {
    Staff staff1 = {"Cordial", "Alicia"};
    Staff staff2 = {"Malardier", "Pierre"};

    cout << "Prenoms des membres du staff :" << endl;
    cout << staff1.prenom << endl;
    cout << staff2.prenom << endl;

    Etudiant etudiant;
    cout << "\nEntrez le nom de l'etudiant : ";
    cin >> etudiant.nom;
    cout << "Entrez le prenom de l'etudiant : ";
    cin >> etudiant.prenom;

    cout << "\nPrenom de l'etudiant : " << etudiant.prenom << endl;

    return 0;
}
